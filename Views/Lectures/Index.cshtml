@model IEnumerable<paSystem.Models.Lecture>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.lectureName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.staffID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.lectureName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.staffID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id }) |
            @Html.ActionLink("PA List", "pasList", new { id = item.Id }) |
            @Html.ActionLink("Add Student", "StudentList", new { id = item.Id })
        </td>
    </tr>
}

</table>
